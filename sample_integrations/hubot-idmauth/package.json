{
  "name": "hubot-idmauth",
  "description": "A hubot script that does the things",
  "version": "0.0.0",
  "keywords": "hubot, hubot-scripts, hubot-enterprise",
  "repository": {
    "type": "git",
    "url": "git://github.com/HPSoftware/hubot-idmauth.git"
  },
  "bugs": {
    "url": "https://github.com/HPSoftware/hubot-idmauth/issues"
  },
  "dependencies": {
    "bluebird": "^3.3.4",
    "hubot-enterprise": "github:eedevops/hubot-enterprise#dev"
  },
  "peerDependencies": {
    "hubot": "2.x"
  },
  "devDependencies": {
    "hubot-test-helper": "1.5.0",
    "hubot-enterprise": "github:eedevops/hubot-enterprise",
    "chai": "^2.1.1",
    "coffee-script": "1.6.3",
    "hubot": "2.x",
    "jshint": "^2.9.2",
    "coffeelint": "^1.15.7",
    "jenkins-mocha": "^2.6.0"
  },
  "main": "index.coffee",
  "scripts": {
    "coffeelint": "coffeelint --reporter jslint -r . > coffee-lint.xml || true",
    "jslint": "jshint --exclude-path .gitignore --reporter=jslint .> js-lint.xml || true",
    "test": "ARTIFACTS_DIR='./' COVERAGE_DIR='./' TEST_DIR='./test/' jenkins-mocha --compilers \"coffee:coffee-script\" --check-leaks --no-coverage test/**/*.coffee || true",
    "lint": "coffeelint -r .; jshint --exclude-path .gitignore . || true"
  },
  "coffeelintConfig": {
    "max_line_length": {
      "limitComments": false
    }
  },
  "author": "Ricardo Quintana <ricardo.quintana@hpe.com>",
  "license": "MIT"
}
